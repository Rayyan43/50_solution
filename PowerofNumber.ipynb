    "def CalculatePower(num,pow):\n",
    "    res = 1\n",
    "    for i in range(0,pow):\n",
    "        res = res*num\n",
    "    return res\n",
    "\n",
    "number_input = int(input(\"Enter a number: \"))\n",
    "power_input = int(input(\"Enter the power: \"))\n",
    "result = CalculatePower(number_input,power_input)\n",
    "print(result)"
