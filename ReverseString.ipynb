    "user_input = input(\"Enter a string\")\n",
    "\n",
    "def revStr(str):\n",
    "    revStr= \"\"\n",
    "    for char in str:\n",
    "        revStr = char +revStr\n",
    "    return revStr\n",
    "\n",
    "result = revStr(user_input)\n",
    "print(result)"
