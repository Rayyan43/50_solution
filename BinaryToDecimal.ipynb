   "source": [
    "def binary_to_decimal(binary_str):\n",
    "    decimal = 0\n",
    "    power = 0\n",
    "    for digit in reversed(binary_str):\n",
    "        decimal = decimal + int(digit) * (2 ** power)\n",
    "        power = power + 1\n",
    "    return decimal\n",
    "\n"
   ]
  }
